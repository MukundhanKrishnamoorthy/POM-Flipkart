<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite thread-count="3" parallel="tests" name="AmazonSuite">

	<test name="chrometest">
		<parameter name="browser" value="chrome">
		</parameter>
		<classes>
			<class name="Pages.ChromeExecution" />
		</classes>
	</test>

	<test name="firefoxtest">
		<parameter name="browser" value="firefox">
		</parameter>
		<classes>
			<class name="Pages.FirefoxExecution" />
		</classes>
	</test>

	<test name="edgetest">
		<parameter name="browser" value="edge">
		</parameter>
		<classes>
			<class name="Pages.EdgeExecution" />
		</classes>
	</test>

</suite> <!-- AmazonSuite -->
